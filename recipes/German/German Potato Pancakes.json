    "recipeIngredient": [
        "2 large eggs",
        "2 tablespoons all-purpose flour",
        "0.25 teaspoon baking powder",
        "0.5 teaspoon salt",
        "0.25 teaspoon pepper",
        "6 medium potatoes, peeled and shredded",
        "0.5 cup finely chopped onion",
        "0.25 cup vegetable oil"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/lQGuVxJQw7_K9HCoThCtfDM2wy4=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/33118-german-potato-pancakes-DDMFS-step-01-d64cd272e31b4372bd434f02ecfedbad.jpg"
                }
            ],
            "text": "Gather all ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/kGHFGYOBZYviXDzyl_WArs2wrE8=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/33118-german-potato-pancakes-DDMFS-step-02-bf8d6ed93bbc4f99896f0978d0af1378.jpg"
                }
            ],
            "text": "Beat eggs, flour, baking powder, salt, and pepper together in a large bowl; stir in potatoes and onion."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/YPAKJlRMxzAvRYzDLIANK688pHY=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/33118-german-potato-pancakes-DDMFS-step-03-cfe210779af84631a74792d21523ceda.jpg"
                }
            ],
            "text": "Heat oil in a large skillet over medium heat. Drop heaping tablespoonfuls of potato mixture into hot oil in batches. Press to flatten."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/_zmmtzZMMucMd1UP3IqYx-1BJ1Y=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/33118-german-potato-pancakes-DDMFS-step-04-eec1a90662564798a8605c2580d40352.jpg"
                }
            ],
            "text": "Cook until browned and crisp, about 3 minutes on each side."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/BrLKBngEI5XbFtl5loo5A3j9gds=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/33118-german-potato-pancakes-DDMFS-step-05-678a1965a81c4802b5ba646e22cfa8ef.jpg"
                }
            ],
            "text": "Transfer to a paper towel-lined plate to drain."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/CTPwMDYbMs3uWaIkdqzQRNtwlno=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/33118-german-potato-pancakes-DDMFS-beauty-4x3-d67678bd232c4ea5929bd495a5e9135d.jpg"
