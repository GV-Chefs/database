    "recipeIngredient": [
        "1 pound lean ground beef",
        "1 cup fresh bread crumbs",
        "1 tablespoon dried parsley",
        "1 tablespoon grated Parmesan cheese",
        "0.25 teaspoon ground black pepper",
        "0.125 teaspoon garlic powder",
        "1 egg, beaten",
        "0.75 cup chopped onion",
        "5 cloves garlic, minced",
        "0.25 cup olive oil",
        "2 (28 ounce) cans whole peeled tomatoes",
        "2 teaspoons salt",
        "1 teaspoon white sugar",
        "1 bay leaf",
        "1 (6 ounce) can tomato paste",
        "0.75 teaspoon dried basil",
        "0.5 teaspoon ground black pepper"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/FooBDuwpGZ4U_-0lBUfTYhC5dpI=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/21353-italian-spaghetti-sauce-with-meatballs-step-1-105-fc73877080c9445390b9e60770a97619.jpg"
                }
            ],
            "text": "Gather all ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/BBEA0d-sgOE9csQnv4Bn6dLsmwE=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/21353-italian-spaghetti-sauce-with-meatballs-step-2-108-bb26498aff7d4ba3be12a5e67bea3539.jpg"
                }
            ],
            "text": "In a large bowl, combine ground beef, bread crumbs, parsley, Parmesan, 1/4 teaspoon black pepper, garlic powder and beaten egg. Mix well and form into 12 balls. Store, covered, in refrigerator until needed."
        },
        {
            "@type": "HowToStep",
            "text": "In a large saucepan over medium heat, saute onion and garlic in olive oil until onion is translucent."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/AKFF8MBomgKqTQgmaiVclaaI-FA=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/21353-italian-spaghetti-sauce-with-meatballs-step-3-13-fe90f8e94412409b954283c2e7f4fce7.jpg"
                }
            ],
            "text": "Stir in tomatoes, salt, sugar and bay leaf. Cover, reduce heat to low, and simmer 90 minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/zH6CVNzG-2HNUIpkznjwJA6Sj3Y=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/21353-italian-spaghetti-sauce-with-meatballs-step-4-22-77349f672c86455d872120a4434de95f.jpg"
                }
            ],
            "text": "Stir in tomato paste, basil, 1/2 teaspoon pepper and meatballs and simmer 30 minutes more."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/ZO5edyo6JhS1ZFELyDepR0Y170w=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/21353-italian-spaghetti-sauce-with-meatballs-DDMFS-4x3-a9a1528b4f06483dbec38d0c2945c378.jpg"
                }
            ],
            "text": "Serve hot and enjoy!"
        }
    ],
    "recipeYield": [
        "6"
    ]
