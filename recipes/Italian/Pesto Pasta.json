    "recipeIngredient": [
        "1 (16 ounce) package pasta",
        "2 tablespoons olive oil",
        "0.5 cup chopped onion",
        "2.5 tablespoons pesto",
        "salt to taste",
        "ground black pepper to taste",
        "2 tablespoons grated Parmesan cheese"
    ],
