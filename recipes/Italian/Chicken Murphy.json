    "recipeCategory": [
        "Dinner"
    ],
    "recipeIngredient": [
        "1 pound Italian sausage, cut into 1/2 inch pieces",
        "1 pound skinless, boneless chicken breast halves - cut into cubes",
        "1 cup all-purpose flour",
        "4 tablespoons olive oil",
        "1 pound cremini mushrooms",
        "1 large onion, diced",
        "1 red bell pepper, cut into 3/4-inch pieces",
        "1 green bell pepper",
        "3 cloves garlic, minced",
        "4 large potatoes",
        "1 cup white wine",
        "1 cup reduced-sodium chicken broth",
        "1/4 cup brine from cherry peppers",
        "1 teaspoon Italian seasoning",
        "3/4 teaspoon salt",
        "1 cup sliced hot cherry peppers",
        "chopped fresh Italian parsley"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/jCtF3pLtrTwbU_-QbdQk0KE1HsU=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/34870-chicken-murphy-ddmfs-step01-4dd7d22a95fa4ac59ec9835dd4f4319b.jpg"
                }
            ],
            "text": "Gather all ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/3dZN6c8RSINhL_8WSvllWO5KoPM=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/34870-chicken-murphy-ddmfs-step03-e0434e61c29046989f6e1cc561f69b80.jpg"
                }
            ],
            "text": "Cook sausage in a 5-to 6-quart Dutch oven over medium heat until browned, 6 to 8 minutes; transfer to a bowl with a slotted spoon."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/NVmcGcDg7w-YHfERcp-LSVM4kAc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/34870-chicken-murphy-ddmfs-step04-8869ad707a4a4ea5a9bb7204224c485c.jpg"
                }
            ],
            "text": "Place flour in a shallow plate; dredge chicken pieces in flour to coat."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/avUWs72qsNTPzECyGNJR5WzFtgs=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/34870-chicken-murphy-ddmfs-step05-811a04d240f6444782b9540d8881226a.jpg"
                }
            ],
            "text": "Heat 1 tablespoon oil in Dutch oven; add chicken and cook until lightly browned, 4 to 5 minutes. Transfer chicken to the bowl with sausage."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/NtLZw8AVWqoQFyyje3ws-3nij0A=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/34870-chicken-murphy-ddmfs-step06-da62400566c84b3c82347c0378da7c48.jpg"
                }
            ],
            "text": "Add remaining 1 tablespoon oil to the Dutch oven. Add mushrooms, onion, and bell peppers. Cook until tender, 6 to 8 minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/tmHanMjpIYRJ7pjn4UTCnfUuba4=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/34870-chicken-murphy-ddmfs-step07-3e31b4078da743e5a6793be22d47bf54.jpg"
                }
            ],
            "text": "Stir in garlic and cook until fragrant, about 1 minute."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/nSP-W6-CToIGMqdWQ_7TJONLAYA=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/34870-chicken-murphy-ddmfs-step09-6258c576276a4837b907f00f1e6b7d4a.jpg"
                }
            ],
            "text": "Stir in potatoes, white wine, chicken broth, brine, Italian seasoning, and salt; bring to a boil. Reduce heat to medium-low, cover, and simmer until potatoes are tender, about 10 minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/aLX4V-xpw1uRLU8AZrjjasYc_Hc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/34870-chicken-murphy-ddmfs-step10-552eb76cffec4bee84b4e33c67ec088b.jpg"
                }
            ],
            "text": "Add Peppadew peppers and browned chicken and sausage; cook and stir until heated through and sauce is slightly thickened, about 3 minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/ogNbFE6TUebDn6iTo13HDIrCjIE=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/34870-chicken-murphy-ddmfs-step11-b94242e2e9e347aabb82e4ed0ff3ba80.jpg"
                }
            ],
            "text": "Sprinkle with parsley before serving."
        }
    ],
    "recipeYield": [
        "8"
    ]
}