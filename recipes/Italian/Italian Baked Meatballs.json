    "recipeIngredient": [
        "1 cup Italian-seasoned bread crumbs",
        "0.25 cup grated Romano cheese",
        "2 tablespoons chopped fresh parsley",
        "0.5 teaspoon salt",
        "0.5 teaspoon ground black pepper",
        "0.5 teaspoon garlic powder",
        "0.5 teaspoon onion powder",
        "0.5 cup water",
        "2 eggs, beaten",
