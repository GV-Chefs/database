    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 375 degrees F (190 degrees C). Line a baking sheet with aluminum foil; coat with cooking spray."
        },
        {
            "@type": "HowToStep",
            "text": "Place salmon fillets on foil; sprinkle with basil. Top with tomato slices, drizzle with olive oil, and sprinkle with Parmesan cheese."
        },
