    "recipeIngredient": [
        "4 tablespoons unsalted butter",
        "2 cups chopped onions",
        "1 pound fresh mushrooms, sliced",
        "2 cups chicken broth",
        "1 tablespoon soy sauce",
        "1 tablespoon paprika",
        "2 teaspoons dried dill weed",
        "1 cup milk",
        "3 tablespoons all-purpose flour",
        "0.5 cup sour cream",
        "0.25 cup chopped fresh parsley",
        "2 teaspoons lemon juice",
        "1 teaspoon salt",
        "ground black pepper to taste"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/ONdaDh_TvZ6yTUex6xNtvEVjGjs=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/17897-hungarian-mushroom-soup-ddmfs-step-1-124-73f4e1f98b404fb0b517dd67fc2acdf6.jpg"
                }
            ],
            "text": "Gather the ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/__sQw-okn7Ul1-IafupYJ0tzZMc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/17897-hungarian-mushroom-soup-ddmfs-step-2-129-bf39561d44b84199a03028d9035bace7.jpg"
                }
            ],
            "text": "Melt butter in a large pot over medium heat. Add onions; cook and stir until softened, about 5 minutes. Add mushrooms and saut\u00e9 for 5 more minutes. Stir in broth, soy sauce, paprika, and dill; reduce heat to low, cover, and simmer for 15 minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/nIFFpaW0JFj4x4vM96XCuKBWkp4=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/17897-hungarian-mushroom-soup-ddmfs-step-3-131-25cce193f3204eaa90a5baec2becf025.jpg"
                }
            ],
            "text": "Whisk milk and flour together in a separate bowl; stir into soup until blended. Cover and simmer for 15 more minutes, stirring occasionally."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/5TQHvAU0DIMmWhCglQA_tvRO9Xg=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/17897-hungarian-mushroom-soup-ddmfs-step-4-135-24510e695f7f4c45812ebbf825b864c1.jpg"
                }
            ],
            "text": "Add sour cream, parsley, lemon juice, salt, and ground black pepper; stir over low heat until warmed through, about 3 to 5 minutes. Serve immediately."
        }
    ],
    "recipeYield": [
        "6"
    ]
}