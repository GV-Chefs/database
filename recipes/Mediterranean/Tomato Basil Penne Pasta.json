                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/cSL5VdXjGxAREOK7FuRvsV17G44=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-193187-tomato-basil-penne-pasta-Beauty-4x3-fe8a68f4b54049b0b17a6bdf3f85a5c0.jpg"
                }
            ],
            "text": "Season with fresh basil."
        }
