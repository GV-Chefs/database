    "recipeIngredient": [
        "1 egg",
        "3 tablespoons mayonnaise",
        "1 tablespoon minced green onions",
        "4 teaspoons lemon juice",
        "1 teaspoon dried tarragon",
        "0.125 teaspoon red pepper flakes",
        "8 ounces crabmeat",
        "0.5 cup crushed buttery round crackers",
        "1 tablespoon butter"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/2LXZQ7kKto28_bkAJB5IVM1w2MU=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-82347-best-ever-crab-cake-ddmfs-step-1-07aae59202024dc1a950ced054967b8b.jpg"
                }
            ],
            "text": "Gather all ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/WuQiBCIxQqFtLc0aLwERksqaq6I=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-82347-best-ever-crab-cake-ddmfs-step-2-9b5896e61e80421f9fcf4ae484a6fff2.jpg"
                }
            ],
            "text": "Whisk egg, mayonnaise, green onions, lemon juice, tarragon, and pepper flakes together in a medium bowl."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/J5rn4B75EcUxnPB2jaicNpBO7jc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-82347-best-ever-crab-cake-ddmfs-step-3-73da78eebb6d4906b9894c92c9335b3c.jpg"
                }
            ],
            "text": "Gently stir in crabmeat, being careful not to break up meat."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/mq39H-leth9JjI0zcLRyUJRXsJI=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-82347-best-ever-crab-cake-ddmfs-step-4-b16c10a8df244c42baf03997889390a6.jpg"
                }
            ],
            "text": "Gradually mix in cracker crumbs, adding until desired consistency is achieved."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/rsv1raxoh_YeQJdswvi_2-l_cX4=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-82347-best-ever-crab-cake-ddmfs-step-4-1-3c2997e72f9140d1854fdec8b774085d.jpg"
                }
            ],
            "text": "Form crab mixture into 4 patties."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
