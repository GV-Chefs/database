    "recipeIngredient": [
        "4 tablespoons soy sauce",
        "4 tablespoons Worcestershire sauce",
        "2 tablespoons liquid smoke flavoring",
        "1 tablespoon ketchup",
        "0.5 teaspoon salt",
        "0.25 teaspoon pepper",
        "0.25 teaspoon garlic powder",
        "0.25 teaspoon onion salt",
        "1 pound boneless venison roast"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/hvGq9m8k6wbkd4_GdERrkCAptbo=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-46324-deer-jerky-DDMFS-step-1-dd3f3a2aed604e6e968c1dd0e8a9dac4.jpg"
                }
            ],
            "text": "Gather all ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/qimx6YNuOTSm5ua8g93vsyjwBwE=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-46324-deer-jerky-DDMFS-step-2-07226e23f52945c6bb0b1ac8dd53a6e2.jpg"
                }
            ],
            "text": "Combine soy sauce, Worcestershire sauce, liquid smoke, ketchup, salt, pepper, garlic powder, and onion salt in a large releasable plastic bag; set aside."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/IrlDL5TL9qIJTFsts-ctgX-gIUE=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-46324-deer-jerky-DDMFS-step-3-ec71937a989a413d92a3ba8bb51fa62d.jpg"
                }
            ],
            "text": "Slice venison into long strips, 1-inch wide and 1/8-inch thick. Add strips to marinade and seal the bag. Refrigerate 8 hours to overnight, kneading occasionally to evenly distribute marinade."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/RURHrsk2Z2eggJE5o2J0TbxN5vw=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-46324-deer-jerky-DDMFS-step-4-1-fa9d6da6630943af90782c6dd7d2e79e.jpg"
                }
            ],
            "text": "Preheat the oven to 160 degrees F (70 degrees C). Place a pan or aluminum foil on the oven floor to catch drippings. Remove venison from the marinade and shake off excess."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/lM7OeBsDvWnoT4nUkO-FKDiNh1Q=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-46324-deer-jerky-DDMFS-step-5-f5d12d3d1350499e8bedafc30a5ddcd1.jpg"
                }
            ],
            "text": "Arrange venison on a wire rack in a single layer and dehydrate in the preheated oven until desired texture is achieved, 6 to 8 hours."
        }
    ],
    "recipeYield": [
        "8"
    ]
}