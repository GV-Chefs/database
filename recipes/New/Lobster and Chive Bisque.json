    "recipeIngredient": [
        "1 cup lobster meat",
        "2 tablespoons minced onion",
        "5 tablespoons butter",
        "4 tablespoons all-purpose flour",
        "3 cups milk",
        "1.5 teaspoons salt",
        "1 cup heavy whipping cream",
        "3 tablespoons chopped fresh chives"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a medium size pot, saute lobster meat, onion and butter until onions are soft."
        },
        {
            "@type": "HowToStep",
            "text": "Stir flour into the pot. When all ingredients are well blended, add milk, salt and cream. Cook over a low heat until soup has thickened."
        },
        {
            "@type": "HowToStep",
            "text": "Garnish soup with green onions before serving."
        }
    ],
    "recipeYield": [
        "6"
    ]
