    "recipeIngredient": [
        "0.5 cup butter",
        "0.66666668653488 cup white sugar",
        "2 large eggs",
        "1 cup buttermilk",
        "0.5 teaspoon baking soda",
        "1 cup cornmeal",
        "1 cup all-purpose flour",
        "0.25 teaspoon salt"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/-mTrpe8gs-nY6tK2niXTGki2UR0=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/76594-grandmothers-buttermilk-cornbread-step1-3x4-0135064414801589-5971cdfc76b2468c9269c16c1db3c97c.jpg"
                }
            ],
            "text": "Gather all ingredients."
        },
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 375 degrees F (175 degrees C). Grease an 8-inch square pan."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/bPqbMTDPpEyeRD5fVUIrRyHYynI=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/76594-grandmothers-buttermilk-cornbread-step3-3x4-0135064414801596-97ebef9f658347f4987b1f6614e4af9e.jpg"
                }
            ],
            "text": "Melt butter in a large skillet over medium heat. Remove from heat and stir in sugar. Quickly whisk in eggs."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/m4dERV6RUF1OiGoopUgaKnnmQck=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/76594-grandmothers-buttermilk-cornbread-step5-1-3x4-0135064414801602-f1406b7abc2341259151093e69830119.jpg"
                }
            ],
            "text": "Combine buttermilk and baking soda in a small bowl; whisk mixture into the skillet. Stir in cornmeal, flour, and salt until well-blended and only a few lumps remain."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/XBPGDvY-ZNauk-AccAseSyvT1kc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/76594-grandmothers-buttermilk-cornbread-step5-2-3x4-0135064414801604-67fa19694d8644d0a6c3034117160aeb.jpg"
                }
            ],
            "text": "Pour batter into the prepared pan."
        },
        {
            "@type": "HowToStep",
            "text": "Bake in the preheated oven until a toothpick inserted in the center comes out clean, 30 to 40 minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/2JmlEafcVbOJpFu4wFUGvUxMNWQ=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/76594-grandmothers-buttermilk-cornbread-beauty-3x4-0135064414801604-dd84dcaf1d0f421eb3b6fdced8cf0ad3.jpg"
                }
            ],
            "text": "Serve and enjoy!"
        }
    ],
    "recipeYield": [
        "9"
    ]
}