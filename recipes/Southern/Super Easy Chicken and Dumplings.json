    "recipeIngredient": [
        "2 (10.5 ounce) cans condensed cream of chicken soup",
        "3 (14 ounce) cans chicken broth",
        "3 cups shredded cooked chicken meat",
        "2 (10 ounce) cans refrigerated biscuit dough"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/v0PN1BsWXwNYW6wpYYHAu44_h7c=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/218562-Super-Easy-Chicken-Dumplings-DDMFS-Step-1-0621-74c21773ef3d44a2b35c5312ed473118.jpg"
                }
            ],
            "text": "Gather all ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/cJdJeg70SRtPySQWYEOp6TObkDk=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/218562-Super-Easy-Chicken-Dumplings-DDMFS-Step-2-0627-3584fa097c1f4f93abb0de7b1d414827.jpg"
                }
            ],
            "text": "Stir condensed soup, chicken broth, and shredded chicken together in a large saucepan over medium-high heat until it begins to simmer."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/ESY-WrzYKNAnIMd1Jyh4uCnv6Gk=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/218562-Super-Easy-Chicken-Dumplings-DDMFS-Step-3-0636-c51c03d64a9540a2af824ca9c217db6d.jpg"
                }
            ],
            "text": "Cut each biscuit into quarters, and gently stir into the simmering soup."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/ovfQhMYkPp2YXS0vY2DSUmQCb8M=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/218562-Super-Easy-Chicken-Dumplings-DDMFS-Step-4-0640-7b17a51f30da40faa5679a0b71132cc0.jpg"
                }
            ],
            "text": "Reduce heat to medium-low, cover, and simmer until biscuits are no longer doughy in the center, 10 to 15 minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/DjIUlrazrP_4S2xnZ9bXc35csts=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/218562-Super-Easy-Chicken-Dumplings-DDMFS-3x4-0647-66859426562849a89d4d7d20c47fd533.jpg"
                }
            ],
            "text": "Serve and enjoy!"
        }
    ],
