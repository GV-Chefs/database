    "recipeIngredient": [
        "1 cup light corn syrup",
        "0.66666698455811 cup white sugar",
        "0.33333298563957 cup margarine, melted",
        "3 large eggs",
        "0.5 teaspoon salt",
        "1.5 cups semisweet chocolate chips",
        "1 cup pecan halves",
        "1 (9 inch) unbaked pie crust"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 375 degrees F (190 degrees C)."
        },
        {
            "@type": "HowToStep",
            "text": "Combine corn syrup, sugar, melted margarine, eggs, and salt in a mixing bowl; beat with an electric mixer until combined. Stir in chocolate chips and pecans. Pour mixture into pie crust."
        },
        {
            "@type": "HowToStep",
            "text": "Bake in the preheated oven until set, 40 to 50 minutes."
        },
        {
            "@type": "HowToStep",
            "text": "Cool completely before serving."
        }
    ],
