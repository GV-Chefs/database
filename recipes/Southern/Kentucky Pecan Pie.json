    "recipeIngredient": [
        "1 cup white corn syrup",
        "1 cup packed brown sugar",
        "0.33333334326744 teaspoon salt",
        "0.33333334326744 cup unsalted butter, melted",
        "3 large eggs, slightly beaten",
        "1 cup chopped pecans",
        "1 (9 inch) unbaked pie crust"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "@type": "HowToStep",
            "text": "Mix together corn syrup, sugar, melted butter, and salt in a large bowl. Add eggs and beat until well combined. Pour mixture into the pie crust and sprinkle pecans over top."
        },
        {
            "@type": "HowToStep",
            "text": "Bake in the preheated oven for 50 to 60 minutes."
        },
        {
            "@type": "HowToStep",
