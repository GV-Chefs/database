{
    "headline": "Southern Fried Chicken Livers",
    "name": "Southern Fried Chicken Livers",
    "description": "These crispy fried chicken livers are seasoned with garlic powder and ready to serve in just 20 minutes for a popular old-time Southern treat!",
    "recipeCuisine": [
        "Southern"
    ],
    "totalTime": "PT30M",
    "cookTime": "PT15M",
    "nutrition": {
        "@type": "NutritionInformation",
        "calories": "470 kcal",
        "carbohydrateContent": "28 g",
        "cholesterolContent": "459 mg",
        "fiberContent": "1 g",
        "proteinContent": "24 g",
        "saturatedFatContent": "5 g",
        "sodiumContent": "87 mg",
        "sugarContent": "2 g",
