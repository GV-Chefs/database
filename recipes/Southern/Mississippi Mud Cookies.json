    "recipeIngredient": [
        "2 cups white sugar",
        "0.5 cup butter",
        "0.5 cup milk",
        "3 cups rolled oats",
        "6 tablespoons unsweetened cocoa powder",
        "0.5 cup chopped walnuts",
        "0.5 cup flaked coconut",
        "1 teaspoon vanilla extract"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Bring sugar, butter, and milk to a boil in a large saucepan. Boil for 2 minutes and remove from heat."
        },
        {
            "@type": "HowToStep",
            "text": "Stir in cocoa and vanilla. Stir in oats, walnuts, and coconut until well combined."
        },
        {
            "@type": "HowToStep",
            "text": "Drop tablespoonfuls onto waxed paper. Cool for 30 minutes before serving."
        }
    ],
