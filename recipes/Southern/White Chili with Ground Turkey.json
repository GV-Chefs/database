    "recipeIngredient": [
        "1.5 pounds ground turkey",
        "1 onion, chopped",
        "3 cloves garlic, minced",
        "2 (4 ounce) cans canned green chile peppers, chopped",
        "1 tablespoon ground cumin",
        "1 tablespoon dried oregano",
        "1 teaspoon ground cinnamon",
        "ground cayenne pepper to taste",
        "ground white pepper to taste",
        "5 cups chicken broth",
        "3 (15 ounce) cans cannellini beans",
        "2 cups shredded Monterey Jack cheese"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/wy_v63DcPIEZhyjy8KD3I4pJZGs=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-17023-White-Chili-with-Ground-Turkey-ddmfs-step-0-c1992166a244438da5cc11018a84b088.jpg"
                }
            ],
            "text": "Gather all ingredients."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/lbY5Y2gk8ZDQb-jImO6Uc_lQtyE=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-17023-White-Chili-with-Ground-Turkey-ddmfs-step-2-88e10f07ec114b2e891feb9eb2ae395e.jpg"
                }
            ],
            "text": "Combine ground turkey, onion, and garlic in a large pot over medium heat; cook and stir until turkey is browned, about 10 minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/GOgZpfQvJxsSeITTZI6aLZLYFKo=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-17023-White-Chili-with-Ground-Turkey-ddmfs-step-3-47d0cc41dd1e48d0ba56afe8bdb97aa0.jpg"
                }
            ],
            "text": "Add chile peppers, cumin, oregano, cinnamon, cayenne pepper, and white pepper; cook and stir 5 more minutes."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/dHItVJ7EoYoaaM6xpIb5_l86OQA=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-17023-White-Chili-with-Ground-Turkey-ddmfs-step-4-ca1d13ace3fe4597b4bd5df686337893.jpg"
                }
            ],
            "text": "Pour in chicken broth and 2 cans of cannellini beans."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/0zHMfIyPRDtp41Rm7grdRu2t0Dc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-17023-White-Chili-with-Ground-Turkey-ddmfs-step-6-1fcc9c4b12cf4e9e8202f9ecbe22bfe5.jpg"
                }
            ],
            "text": "Puree the remaining can of beans in a blender or food processor; stir into the soup and add cheese."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/B_7tAW-3Z1ku7JnZuabhhQOd7F0=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/AR-17023-White-Chili-with-Ground-Turkey-ddmfs-4x3-1bebac59c8d84f869220640fdeba4485.jpg"
                }
            ],
            "text": "Simmer for 10 minutes, stirring occasionally, until cheese has blended into the soup. Serve and enjoy!"
        }
