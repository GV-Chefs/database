    "recipeIngredient": [
        "1.5 cups beer",
        "1 cup all-purpose flour",
        "1 large egg",
        "1 teaspoon garlic powder",
        "0.5 teaspoon salt",
        "0.5 teaspoon ground black pepper",
        "1 pound cod fillets",
        "2 cups crushed cornflake crumbs",
        "1 teaspoon Cajun seasoning",
        "1 quart oil for frying"
