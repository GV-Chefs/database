    "recipeIngredient": [
        "cooking spray",
        "1 tablespoon Cajun seasoning",
        "1 teaspoon brown sugar",
        "2 (6 ounce) skin-on salmon fillets"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/Iwy2AJ353xUoL9y5l90ZLqB1l_o=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/275343-Cajun-Air-Fryer-Salmon-DDMFS-Step-1-0f509260b09a49d19376bdcbd28294b3.jpg"
                }
            ],
            "text": "Gather all ingredients. Preheat the air fryer to 390 degrees F (200 degrees C)."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/ZHY_9BDeYhD5AMEkCAXI9zuzziM=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/275343-Cajun-Air-Fryer-Salmon-DDMFS-Step-2-cc91b705c09f47a3b1700727d5ff6b52.jpg"
                }
            ],
            "text": "Rinse and dry salmon fillets with a paper towel. Mist fillets with cooking spray."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/11fhCRjWg6t8YWyU23zkxeNfOEc=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/275343-Cajun-Air-Fryer-Salmon-DDMFS-Step-3-004f3a8bc5114fa8aedef776c2af7056.jpg"
                }
            ],
            "text": "Mix together Cajun seasoning and brown sugar in a small bowl until combined; spread onto a plate."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/sEDe1rdBOovw3w5bydypLNDJd3k=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/275343-Cajun-Air-Fryer-Salmon-DDMFS-Step-4-8e41a32f082343b488478c1efc3de9be.jpg"
                }
            ],
            "text": "Press fillets, flesh-side down, into seasoning mixture."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/OXetjaKZNZCXQyS0accX2GJfcPg=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/275343-Cajun-Air-Fryer-Salmon-DDMFS-Step-5-3e1a6584794141da8464734e20590a69.jpg"
                }
            ],
            "text": "Spray the basket of the air fryer with cooking spray and place salmon fillets skin-side down. Mist salmon again lightly with cooking spray."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/pUkZTSl_H5K4uthcGSVnMRZh7aA=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/275343-Cajun-Air-Fryer-Salmon-DDMFS-Step-6-967c4f8f63944c18a0bbb97ea36fc653.jpg"
                }
            ],
            "text": "Close the lid and cook in the preheated air fryer for 8 minutes. Remove from the air fryer and let rest for 2 minutes before serving."
        }
    ],
    "recipeYield": [
        "2"
    ]
